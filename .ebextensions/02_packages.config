option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "lmuvote.settings"
    "PYTHONPATH": "/opt/python/current/app/lmuvote:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: lmuvote/lmuvote/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:environment:proxy:staticfiles":
    "/static/": "www/static/"